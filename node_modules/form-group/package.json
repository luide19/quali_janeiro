    "type": "git",
    "url": "git+https://github.com/fabrigeas/vue-formgroup.git"
  },
  "bugs": {
    "url": "https://github.com/fabrigeas/vue-formgroup/issues"
  },
  "homepage": "https://github.com/fabrigeas/vue-formgroup#readme"
}